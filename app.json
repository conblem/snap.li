{
  "expo": {
    "name": "snap.li",
    "description": "Super basic Snapchat clone",
    "slug": "snapli",
    "privacy": "public",
    "sdkVersion": "23.0.0",
    "version": "0.1.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "me.conblem.snapli"
    },
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    }
  }
}